{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "KafkaLogger": {
    "Targets": {
      "Target1": {
        "Topic": "order-log-topic"
      }
    },
    "Rules": [
      {
        "WriteTo": "Target1"
      }
    ],
    "BootstrapServers": "localhost:9092"
  },
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "CommandTopic": "balance-command-topic",
    "PersistentTopic": "order-persistent-topic"
  }
}
